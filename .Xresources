
URxvt.termName:   rxvt-unicode
URxvt.scrollBar:  false
Xft.dpi:          96
Xft.antialias:    true
Xft.rgba:      	  rgb
Xft.hinting:      true
Xft.hintstyle:    hintslight

!! Transparency settings
URxvt.transparent:     false
URxvt.shading:         15
URxvt.depth:           32
!! URxvt.inheritPixmap: true

!! Fonts
URxvt.font:        xft:Cascadia Code:pixelsize=13:antialias=true,xft:Cascadia Code
URxvt.boldFont:    xft:Cascadia Code:pixelsize=13:weight=bold:antialias=true,xft:Cascadia Code
URxvt.letterSpace: -1
